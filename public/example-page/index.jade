          .col-xs-12.col-sm-6.col-md-3.col-lg-2: +content-tile("https://www.wgsn.com/storage/vol21/fo_image_store/cached/boards/67227/thumb.jpg", "Future Trends", "Prints & Graphics Women S/S 18: Silent Shapes – Slow Futures", false, "04.06.16", "Hannah Watkins")
