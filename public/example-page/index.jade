extends ../layout

include ../style-guide/content-tiles/mixins.jade
include ../style-guide/ui-elements/carousel.jade

block append vars
  - bodyclass = 'example-page'

block content
  section
    .example-page--header.bg-black
      .content-wrap
        .row
          .col-xs-12.col-sm-10 &nbsp;
          .col-xs-12.col-sm-2
            .block-ver-sm
              label.label.label--dropdown.tight
                select.input.input--dropdown.tight#product-changer2
                  option(value="wgsn") WGSN
                  option(value="insight") Insight
                  option(value="fashion") Fashion
                  option(value="li") Lifestyle &amp; Interiors
                  option(value="instock") Instock
                  option(value="styletrial") Styletrial

    .row.page-title
      .col-xs-12
        h4.page-title--categories Market Reporting
        h1.page-title--primary Catwalk Analysis

    .row
      .col-xs-12.col-sm-3.col-md-2
        .sidebar-menu
          .sticky
            h4.sidebar-menu--title Future Trends
            ul
              li: a(href="") S/S 18
              li: a(href="") A/W 17/18
              li: a(href="") S/S17
              li: a(href="") A/W 16/17
              li: a(href="") S/S 16
              li: a(href="") A/W 15/16

            h4.sidebar-menu--title Market Reporting
            ul
              li.sidebar-menu--active: a(href="") Catwalk Analysis
              li: a(href="") Trade Shows
              li: a(href="") Retail &amp; Buying

            h4.sidebar-menu--title Trend Watch
            ul
              li: a(href="") Colour Alerts
              li: a(href="") Inspiration

      .col-xs-12.col-sm-9.col-md-10
        .row.block-top-md.block-bot-lg
          .col-xs-12.col-sm-9
            .swiper-container
              .swiper-wrapper
                +carousel-slide("https://www.wgsn.com/storage/vol21/fo_image_store/cached/boards/67398/thumb.jpg", "Retail", "New Look A/W 16/17 – Womenswear", "03.08.16", "Faye Howard")
                +carousel-slide("https://www.wgsn.com/storage/vol21/fo_image_store/cached/boards/66680/thumb.jpg", "Trend Forecast", "Women's Forecast S/S 18: Slow Futures", "01.08.16", "Robbie Sinclair, Jane Boddy, Laura Yiannakou")
                +carousel-slide("https://www.wgsn.com/storage/vol21/fo_image_store/cached/boards/67015/thumb.jpg", "Trade Shows", "Berlin Trade Shows S/S 17 – Premium", "08.07.16", "Laura Yiannakou")
                +carousel-slide("https://www.wgsn.com/storage/vol21/fo_image_store/cached/boards/67227/thumb.jpg", "Future Trends", "Prints & Graphics Women S/S 18: Silent Shapes – Slow Futures", "04.06.16", "Hannah Watkins")
                +carousel-slide("https://www.wgsn.com/storage/vol21/fo_image_store/cached/boards/67589/thumb.jpg", "Visual Merchandising", "S/S 17 Menswear & A/W 16/17 Couture Catwalk Sets", "08.08.16", "Claire Dickinson")
                +carousel-slide("https://www.wgsn.com/storage/vol21/fo_image_store/cached/boards/67601/thumb.jpg", "Knitwear", "The Sporty Crop-Top", "09.08.16", "Laura Yiannakou")

              .swiper-button-prev: i.icon.icon-arrow-left
              .swiper-button-next: i.icon.icon-arrow-right

          .col-xs-12.col-sm-3
            .ad-box.block-xl.bg-lt-grey

